<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	assembly="NHibernate.Test"
	namespace="NHibernate.Test.NHSpecificTest.NH387"
	default-lazy="false">

	<class name="_Parent" table="parent">
		<id name="_Id" column="id" unsaved-value="0">
			<generator class="native" />
		</id>
    <property name="Text" type="string"/>
    <bag name="_Children" cascade="all">
      <key column="parent_id" />
      <one-to-many class="_Child" />
    </bag>
	</class>

	<class name="_Child" table="child">
		<id name="_Id" column="child_id">
			<generator class="native" />
		</id>
	</class>

  <class name="_AssignedIdObject" table="AssignedID">
    <id name="_Id" column="parent_id" unsaved-value="0">
      <generator class="assigned" />
    </id>
    <property name="Text" type="string"/>
  </class>
</hibernate-mapping>
 
